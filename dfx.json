{
  "canisters": {
    "manager": {
      "main": "canisters/src/manager/manager.mo",
      "type": "motoko"
    },
    "assets": {
      "dependencies": ["manager"],
      "frontend": {
        "entrypoint": "studio/www/index.html"
      },
      "source": ["studio/www"],
      "type": "assets",
      "initialization_values": {
        "memory_allocation": "8GB"
      }
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "dfx": "0.9.2",
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}
