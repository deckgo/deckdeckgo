{
  "name": "deckdeckgo",
  "version": "4.10.0",
  "author": "David Dal Busco",
  "description": "The web open source editor for presentations",
  "private": true,
  "scripts": {
    "ic:install:data": "node canisters/scripts/ic.installcode.mjs",
    "ic:types": "node ./canisters/scripts/ic.types.js",
    "build": "npm run ic:types && cd studio && npm run build:ic",
    "postbuild": "prettier --write providers/ic"
  },
  "workspaces": [
    "providers/*",
    "webcomponents/*",
    "templates/*",
    "utils/*"
  ],
  "devDependencies": {
    "@stencil/core": "^2.8.0",
    "@stencil/postcss": "^2.0.0",
    "@stencil/sass": "^1.4.1",
    "@types/jest": "^26.0.24",
    "@types/puppeteer": "5.4.4",
    "autoprefixer": "^10.3.4",
    "esbuild": "^0.12.25",
    "husky": "^7.0.2",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "node-fetch": "^2.6.1",
    "prettier": "2.3.2",
    "pretty-quick": "^3.1.1",
    "puppeteer": "10.2.0",
    "typescript": "^4.4.2"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/deckgo/deckdeckgo.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
