type UserId = principal;
type BucketId = principal;
service : {
  delData: () -> (bool);
  delStorage: () -> (bool);
  getData: () -> (opt BucketId) query;
  getStorage: () -> (opt BucketId) query;
  initData: () -> (BucketId);
  initStorage: () -> (BucketId);
  installCode: (principal, UserId, blob) -> ();
  list: (text) -> (vec record {
                         bucketId: BucketId;
                         owner: UserId;
                       }) query;
}
